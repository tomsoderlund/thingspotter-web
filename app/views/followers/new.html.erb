<h1>New follower</h1>

<% form_for(@follower) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :follower_id %><br />
    <%= f.text_field :follower_id %>
  </p>
  <p>
    <%= f.label :follows_user_id %><br />
    <%= f.text_field :follows_user_id %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', followers_path %>