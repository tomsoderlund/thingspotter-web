<% form_for(@store) do |f| %>
  <%= f.error_messages %>
<table>
	<tr><td><%= f.label :name %></td><td><%= f.text_field :name %></td></tr>
	<tr><td><%= f.label :url_key %></td><td><%= f.text_field :url_key %></td></tr>
	<tr><td><%= f.label :url_data %></td><td><%= f.text_field :url_data %></td></tr>
	<tr><td><%= f.label :body_data %></td><td><%= f.text_area :body_data %></td></tr>
</table>
  <p>
    <%= f.submit 'Save' %>
  </p>
<% end %>